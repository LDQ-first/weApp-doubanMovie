<import src="../search/search.wxml"/>
<import src="../more/more.wxml"/>
<import src="../movie/movie.wxml"/>
<template name="movie-list">
    <view class="page">
        <view class="head">
            <template is="search"/>
            <template is="more"/>
        </view>
        <view class="main">
            <view class="title">
                <text>{{title}}</text>
            </view>
            <template is="movie" data="{{list:list}}"/>
        </view>
    </view>
</template>